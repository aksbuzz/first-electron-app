{
    "name": "first-electron-app",
    "productName": "first-electron-app",
    "version": "1.0.0",
    "description": "My Electron application description",
    "main": "src/index.js",
    "scripts": {
        "start": "electron-forge start",
        "package": "electron-forge package",
        "make": "electron-forge make",
        "publish": "electron-forge publish",
        "lint": "echo \"No linting configured\"",
        "build-installer": "electron-builder "
    },
    "keywords": [],
    "author": {
        "name": "aksbuzz",
        "email": "akshayjr69@gmail.com"
    },
    "build": {
        "appId": "first-electron-app",
        "win": {
            "target": [
                "nsis"
            ],
            "icon": "src/ghex-icon.png",
            "requestedExecutionLevel": "requireAdministrator"
        },
        "nsis": {
            "installerIcon": "src/ghex-icon.png",
            "uninstallerIcon": "src/ghex-icon.png",
            "uninstallDisplayName": "First Electron App",
            "license": "license.txt",
            "oneClick": "false",
            "allowToChangeInstallationDirectory": true
        }
    },
    "license": "MIT",
    "config": {
        "forge": {
            "packagerConfig": {},
            "makers": [
                {
                    "name": "@electron-forge/maker-squirrel",
                    "config": {
                        "name": "first_electron_app",
                        "author": "Akshay"
                    }
                },
                {
                    "name": "@electron-forge/maker-zip",
                    "platforms": [
                        "darwin",
                        "linux"
                    ]
                }
            ],
            "publishers": [
                {
                    "name": "@electron-forge/publisher-github",
                    "config": {
                        "repository": {
                            "owner": "aksbuzz",
                            "name": "first-electron-app"
                        },
                        "draft": true
                    }
                }
            ]
        }
    },
    "dependencies": {
        "auto-launch": "^5.0.5",
        "electron-squirrel-startup": "^1.0.0"
    },
    "devDependencies": {
        "@electron-forge/cli": "^6.0.0-beta.63",
        "@electron-forge/maker-deb": "^6.0.0-beta.63",
        "@electron-forge/maker-rpm": "^6.0.0-beta.63",
        "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
        "@electron-forge/maker-zip": "^6.0.0-beta.63",
        "@electron-forge/publisher-github": "^6.0.0-beta.63",
        "electron": "17.0.1",
        "electron-builder": "^22.14.13",
        "ps-list": "7.2.0"
    }
}
